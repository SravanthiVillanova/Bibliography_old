-- SELECT count(*),title FROM `workattribute_option` where `workattribute_id` = 5 group by(title) having count(title) > 1
-- compare following two queries
SELECT count(DISTINCT(value)),value FROM `work_workattribute` where `workattribute_id` = 5 group by(value) //returned 53 rows
SELECT title,id FROM `workattribute_option` where `workattribute_id` = 5                                   //returned 53 rows
